type Mutation {
  registerFeed(input: RegisterFeedInput!): RegisterFeedPayload!
  deleteFeed(input: DeleteFeedInput!): DeleteFeedPayload!
}

input RegisterFeedInput {
  url: String!
}

type RegisterFeedPayload {
  feedId: ID!
}

input DeleteFeedInput {
  feedId: ID!
}

type DeleteFeedPayload {
  feedId: ID!
}